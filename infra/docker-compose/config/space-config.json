{
    "space": "${KC_REALM}",
    "theme": "${KC_REALM_THEME}",
    "appClients": [
        {
            "clientId": "${KC_APP_CLIENT_ID}",
            "clientSecret": "${KC_APP_CLIENT_SECRET}",
            "roles": [],
            "realmManagementRoles": [
                "manage-users",
                "view-users",
                "query-groups",
                "manage-realm"
            ]
        },{
            "clientId": "tr-registry-script-api",
            "clientSecret": "secret",
            "roles": ["tr_orchestrator_admin"],
            "realmManagementRoles": [
                "manage-users",
                "view-users",
                "query-groups",
                "manage-realm"
            ]
        }
    ],
    "webClients": [
        {
            "clientId": "${KC_WEB_CLIENT_ID}",
            "webUrl": "${REGISTRY_URL_PUBLIC}"
        },
        {
            "clientId": "${KC_CONNECT_ADMIN_WEB_CLIENT_ID}",
            "webUrl": "${ADMIN_URL_PUBLIC}"
        }
    ],
    "permissions": [
        {
            "name": "fs_file_read",
            "description": "Ability to read files from the filesystem."
        },
        {
            "name": "fs_file_write",
            "description": "Ability to write or modify files in the filesystem."
        },
        {
            "name": "fs_policy_write",
            "description": "Ability to write or change policies in the filesystem."
        },
        {
            "name": "tr_project_read",
            "description": "Ability to view project data"
        }, {
            "name": "tr_project_write",
            "description": "Ability to modify project data"
        }
    ],
    "roles": [
        {
            "name": "tr_orchestrator",
            "description": "Main organization role for orchestrators",
            "targets": ["ORGANIZATION"],
            "locale": {
                "fr": "Orchestrateur",
                "en": "Orchestrator"
            },
            "bindings": {
                "USER": ["tr_orchestrator_user", "tr_orchestrator_admin"],
                "API_KEY": ["tr_orchestrator_user", "tr_orchestrator_admin"]
            },
            "permissions": ["feat_im_all", "feat_im_own"]
        },
        {
            "name": "tr_orchestrator_user",
            "description": "Basic user role for orchestrator org",
            "targets": ["USER", "API_KEY"],
            "locale": {
                "fr": "Utilisateur",
                "en": "User"
            },
            "permissions": ["im_user_read", "tr_project_read"]
        },
        {
            "name": "tr_orchestrator_admin",
            "description": "Admin user role for orchestrator org",
            "targets": ["USER", "API_KEY"],
            "locale": {
                "fr": "Administrateur",
                "en": "Administrator"
            },
            "permissions": [
                "fs_file_read","fs_file_write", "fs_policy_write",
                "im_organization_read", "im_organization_write",
                "im_user_read", "im_user_write",
                "im_role_read",  "im_role_write",
                "im_apikey_read", "im_apikey_write",
                "tr_project_read", "tr_project_write",
                "feat_im_all", "feat_im_own"
            ]
        },
        {
            "name": "tr_project_manager_user",
            "description": "Basic user role for project manager org",
            "targets": ["USER", "API_KEY"],
            "locale": {
                "fr": "Utilisateur",
                "en": "User"
            },
            "permissions": ["im_user_read", "im_role_read"]
        },
        {
            "name": "tr_project_manager_admin",
            "description": "Admin user role for project manager org",
            "targets": ["USER", "API_KEY"],
            "locale": {
                "fr": "Administrateur",
                "en": "Administrator"
            },
            "permissions": [
                "im_organization_write_own",
                "im_user_read", "im_user_write",
                "im_role_read",
                "im_apikey_read", "im_apikey_write"
            ]
        },
        {
            "name": "tr_project_manager",
            "description": "Main organization role for project manager",
            "targets": ["ORGANIZATION"],
            "locale": {
                "fr": "Gestionnaire de projet",
                "en": "Project Manager"
            },
            "bindings": {
                "USER": ["tr_project_manager_user", "tr_project_manager_admin"],
                "API_KEY": ["tr_project_manager_user", "tr_project_manager_admin"]
            }
        },
        {
            "name": "tr_stakeholder_user",
